{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Give instance role access to S3 Bucket",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::dh.scripts",
        "arn:aws:s3:::dh.scripts/*"
      ],
      "Condition": {
        "StringNotLike": {
          "aws:userId": [
            "AROATXEB3YK6B4GZFQEXH:*",
            "AIDATXEB3YK6GOZOZPOTX",
            "AIDATXEB3YK6MQELUQ6GJ"
          ]
        }
      }
    }
  ]
}