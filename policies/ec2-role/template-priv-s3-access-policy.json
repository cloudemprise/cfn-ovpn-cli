{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ProjectBucketListObjects",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::ProjectName"
    },
    {
      "Sid": "ProjectBucketSendRecieveObjects",
      "Effect":"Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::ProjectName/*"
    }
  ]
}
