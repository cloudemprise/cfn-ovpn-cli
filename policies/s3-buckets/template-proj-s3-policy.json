{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Instance-role/UserId/Root access to S3 Bucket only",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::ProjectName",
        "arn:aws:s3:::ProjectName/*"
      ],
      "Condition": {
        "StringNotLike": {
          "aws:userId": [
            "RootAccount",
            "ConsoleAdmin",
            "ScriptCallerUserId"
          ]
        }
      }
    }
  ]
}
